<h1>Students</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First name</th>
            <th scope="col">Last name</th>
            <th scope="col">Options</th>
        </tr>
    </thead>
    <tbody>
        <% @students.each do |student| %>
            <tr>
                <td><%= student.id %></td>
                <td><%= student.first_name %></td>
                <td><%= student.last_name %></td>
                <td>
                    <a href="<%= edit_student_path student %>" class="btn btn-warning ml-2">Edit</a>
                    <a href="<%= student_path student %>" class="btn btn-primary mr-2">Voir</a>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>
<a href="<%= new_student_path %>" class="btn btn-primary">New</a>